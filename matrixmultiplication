#include<stdio.h>
int main()
{
  int i,j,r1,r2,c1,c2,k;
  printf("enter no of rows in matrix 1:");
  scanf("%d",&r1);
  printf("enter no of columns in matrix 1:");
  scanf("%d",&c1);
  printf("enter the values of matrix");

  
  int a[r1][c1];
  for(i=0;i<r1;i++)
    {
      for(j=0;j<c1;j++)
        {
          scanf("%d",&a[i][j]);
        }
    }



  
  
  printf("enter no of rows in matrix 2:");
  scanf("%d",&r2);
  printf("enter no of columns in matrix 2:");
  scanf("%d",&c2);
  printf("enter the values of matrix");
  int b[r2][c2];
  for(i=0;i<r1;i++)
    {
      for(j=0;j<c1;j++)
        {
          scanf("%d",&b[i][j]);
        }
    }



  
  int res[r1][c2];
  for(i=0;i<r1;i++)
    {
      for(j=0;j<c2;j++)
        {
          res[i][j]=0;
        }
    }

  for(i=0;i<r1;i++)
  for(j=0;j<c2;j++)
  for(k=0;k<c1;k++)
  {
    res[i][j]=res[i][j]+a[i][k]*b[k][j];
  }
    
  for(i=0;i<r1;i++)
  {
  for(j=0;j<c2;j++)
    {
      printf("%d\t",res[i][j]);
    }
    printf("\n");
  }
    return 0;
}
